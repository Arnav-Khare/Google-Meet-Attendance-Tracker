{
    "name":"Attendance Collector",
    "description":"Attendance Collector For JIIT Faculty",
     "web_accessible_resources": [
        "js/inject.js"
    ],
    "version":"1.0",
    "manifest_version":2,
    "background":{
        "service_worker":"index.js"
    },
    "content_scripts": [
      {
          "matches": ["*://meet.google.com/**-**-**"],
          "js": [
              "js/content.js"
          ]
      }
  ],
    "permissions": [
      "storage",
      "identity",
      "tabs",
      "notifications",
      "*://meet.google.com/**-**-**"
  ],
    "action": {
      "default_popup": "popup.html"
    }
}